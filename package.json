    "@types/cors": "^2.8.6",
    "@types/debug": "0.0.30",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "0.0.44",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.0",
    "@types/nedb": "^1.8.9",
    "@types/node": "^13.7.0",
    "@types/properties-reader": "0.0.1",
    "@types/request": "^2.48.4",
