    "apollo-datasource-rest": "^0.3.2",
    "apollo-server-express": "^2.9.16",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.2.0",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "debug": "^3.2.6",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "generate-password": "^1.5.0",
    "generate-sms-verification-code": "^1.0.5",
    "graphql": "^14.6.0",
    "graphql-request": "^1.8.2",
    "header-case-normalizer": "^1.0.3",
    "helmet": "^3.21.2",
    "install": "^0.12.2",
    "json-typescript-mapper": "^1.1.3",
    "libphonenumber-js": "^1.7.31",
    "locale2": "^2.3.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nedb": "^1.8.0",
    "npm": "^6.13.7",
    "password-generator": "^2.2.3",
    "password-validator": "^4.1.3",
    "path": "^0.12.7",
    "properties-reader": "0.0.16",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rxjs": "^6.5.4",
    "to": "^0.2.9",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typescript-logging": "^0.6.4",
    "underscore": "^1.9.2",
    "update": "^0.7.4",
    "uuid": "^3.4.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
